{
  "name": "v-regexp",
  "version": "1.0.0",
  "description": "JavaScript Regular Expression Parser and Visualizer.",
  "main": "index",
  "repository": {
    "type": "git",
    "url": "https://github.com/yorkyu/v-regexp.git"
  },
  "keywords": [
    "Regular Expression",
    "RegExp",
    "v-regexp"
  ],
  "author": {
    "name": "yorkyu"
  },
  "license": "MIT",
  "scripts": {
    "serve": "cd site && webpack serve --mode development --config webpack.config.js",
    "build": "webpack --mode production --progress",
    "lint": "eslint ./src --max-warnings 0 --ext .ts,.tsx,.js --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "raphael": "^2.3.0",
    "save-svg-as-png": "^1.4.17",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.3",
    "@types/raphael": "^2.3.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.22.0",
    "antd": "^4.15.2",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^5.2.4",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "^8.1.1",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
